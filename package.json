{
  "name": "angular-unocss-xp",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:lib": "ng build --project @unocss/angular",
    "watch": "ng build --watch --configuration development",
    "ssr": "node dist/angular-unocss-xp/server/server.mjs",
